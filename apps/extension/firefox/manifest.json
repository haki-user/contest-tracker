{
  "manifest_version": 2,
  "name": "Contests Tracker",
  "version": "2.0.0",
  "description": "Extension for programming contest details",
  "icons": {
    "16": "gojo.png",
    "48": "gojo.png",
    "128": "gojo.png"
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self';"
}
